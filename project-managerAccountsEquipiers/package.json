    "eslint-plugin-promise": "^7.2.1",
    "jiti": "^2.5.1",
    "rimraf": "^6.0.1",
    "tsx": "^4.19.4",
    "typescript": "^5.6.0",
    "typescript-eslint": "^8.32.0",
    "dayjs": "^1.11.13"
  },
  "dependencies": {
    "@google/clasp": "^2.4.2",
    
    "zx": "^8.1.2"
